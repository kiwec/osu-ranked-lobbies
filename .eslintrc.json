{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "new-cap": "off",
    "no-invalid-this": "off"
  }
}